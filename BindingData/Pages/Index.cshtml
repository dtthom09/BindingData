@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    <center>
        <table border="1">
            <tr>
                <th>ID</th>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Age</th>
            </tr>
            @foreach(var i in Model.Getrecords)
            {
        <tr>
            <td>@i.ID</td>
            <td>@i.FirstName</td>
            <td>@i.LastName</td>
            <td>@i.Age</td>
        </tr>
            }
        </table>
    </center>
</div>
